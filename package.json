{
  "name": "wiki-builder",
  "version": "0.9.9",
  "description": "Build github wikis from markdown files",
  "private": false,
  "main": "lib/build.js",
  "scripts": {
    "start": "node src/wikiBuild.js",
    "build": "rm ./lib/build.js || true && babel src/index.js --out-file lib/build.js",
    "test": "[ -f ./src/index.js ] && [ -f ./src/wikiBuild.js ] && npm run build && [ -f ./lib/build.js ]"
  },
  "bin": {
    "wiki-builder": "src/wikiBuild.js"
  },
  "files": [
    "lib/build.js",
    "src/wikiBuild.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/petekeller2/wiki-builder.git"
  },
  "author": "Peter Keller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/petekeller2/wiki-builder/issues"
  },
  "homepage": "https://github.com/petekeller2/wiki-builder#readme",
  "dependencies": {
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "shelljs": "^0.8.3",
    "tracer": "^0.9.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-airbnb": "^3.2.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0"
  }
}
